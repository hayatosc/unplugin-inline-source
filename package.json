{
  "name": "unplugin-inline-source",
  "version": "0.1.0",
  "description": "Inline external scripts and stylesheets into HTML using the `inline` attribute",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.mjs",
      "require": "./dist/core.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.mjs",
      "require": "./dist/rollup.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.mjs",
      "require": "./dist/esbuild.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.mjs",
      "require": "./dist/rspack.cjs"
    },
    "./rolldown": {
      "types": "./dist/rolldown.d.ts",
      "import": "./dist/rolldown.mjs",
      "require": "./dist/rolldown.cjs"
    },
    "./farm": {
      "types": "./dist/farm.d.ts",
      "import": "./dist/farm.mjs",
      "require": "./dist/farm.cjs"
    },
    "./bun": {
      "types": "./dist/bun.d.ts",
      "import": "./dist/bun.mjs",
      "require": "./dist/bun.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "bun test"
  },
  "dependencies": {
    "unplugin": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "bun-types": "^1.3.8",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "unplugin",
    "vite",
    "rollup",
    "webpack",
    "esbuild",
    "inline",
    "html"
  ]
}
